#operadores aritimeticos (continuação)
valor1 = 8
valor2 = 11
print("exponenciação:",valor1**3)
print("divisão padrão:",valor1/valor2)
print("divisão sem decimais:",valor1//valor2)
print("modulo(resto) da divisão",valor2%3)

#operadores de comparação(relacionais)
print("valor1 é maior que o valor2:", valor1>valor2)
print("valor é maior ou igual a valor2:",valor1>=valor2)
print("valor1 é menor ou igual a valor2:", valor1<=valor2)
print("valor1 é igual a valor2:",valor1==valor2)
print("valor1 é diferente de valor2",valor1!=valor2)
